{
  "editor.fontFamily": "GopherMono-Regular,Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 16,
  "auto-close-tag.excludedTags": [
    "area",
    "base",
    "br",
    "col",
    "command",
    "embed",
    "hr",
    "img",
    "input",
    "keygen",
    "link",
    "meta",
    "param",
    "source",
    "track",
    "wbr"
  ],
  "editor.lineHeight": 1.5,
  "debug.console.fontSize": 16,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.5
}
